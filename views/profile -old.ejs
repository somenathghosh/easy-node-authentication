    <div class="container">
      
      <div class="row-fluid heading">
        <div class="span12">
		<br><br>
          <h1><font color="white">TCS Charlotte lab</font></h1>
        </div>
      </div> 
      <div class="row-fluid subheading">
        <div class="span12">
          
          <p class="lead"><font color="white">Please log the use of Lab Machine</font></p>
        </div>
      </div>
        
      <div class="row-fluid section1">
      
        <div class="span5 copy">
		<form action="/new", method ="POST">
			<center>
			  <table style="height: 253px; margin-left: 60px; width: 696px;">
				<tbody>
				  <tr>
					<td style="width: 379px;"><font face="trebuchet MS" size="5" color="white">Employee ID:</font>
					</td>
					<td style="width: 0px;">
						<% if (user.local.email) { %>
						<p><font face="trebuchet MS" size="5" color="white"><%= user.local.email %></font></p>
						
						<!--<input name="empID" type="number">-->
						<% } %>
					</td>
				  </tr>
				  <tr>
					<td style="width: 379px;"><font face="trebuchet MS" size="5" color="white">Full Name:</font>
					</td>
					<td style="width: 0px;"><input name="name" type="text" placeholder="Your TCS name"></td>
				</tr>
				<tr>
					<td style="width: 379px;"><font face="trebuchet MS" size="5" color="white">Reason why are you here ? </font></td>
					<td style="width: 0px;"><textarea name="reason" type="text" class=resizedTextbox placeholder="why do you want to use the Lab machine ?"></textarea>
					</td>
				</tr>
    </tbody>
  </table>
  </center>

			
          <div class="span8 offset10 order">
            
            <button class="btn btn-success btn-large" type="submit">Register</button>
          </div>

